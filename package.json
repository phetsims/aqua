{
  "name": "aqua",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/aqua.git"
  },
  "devDependencies": {
    "@slack/bolt": "~3.11.0",
    "axios": "~0.26.1",
    "dotenv": "^16.0.3",
    "grunt": "~1.5.3",
    "lodash": "~4.17.10",
    "ncp": "~2.0.0",
    "rimraf": "~2.5.4",
    "puppeteer": "~19.2.2",
    "winston": "~0.9.0"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/node_eslintrc.js",
    "globals": {
      "aqua": "readonly",
      "XMLHttpRequest": "readonly",
      "Hashes": "readonly",
      "phetCore": "readonly",
      "axon": "readonly",
      "dot": "readonly",
      "kite": "readonly",
      "scenery": "readonly",
      "__dirname": "readonly"
    },
    "overrides": [
      {
        "files": [
          "./js/browser-tools/**",
          "./js/client/**",
          "./js/report/**"
        ],
        "extends": "../chipper/eslint/phet-library_eslintrc.js",
        "rules": {
          "bad-sim-text": "off"
        },
        "env": {
          "browser": true,
          "node": false
        }
      },
      {
        "files": [
          "**/*.ts"
        ],
        "parserOptions": {
          "project": [
            "../aqua/tsconfig.json"
          ]
        }
      }
    ]
  },
  "phet": {
    "buildStandalone": true,
    "standaloneTranspiles": [
      "../query-string-machine/js/QueryStringMachine.js"
    ]
  }
}