{
  "name": "aqua",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/aqua.git"
  },
  "devDependencies": {
    "grunt": "~1.0.0",
    "lodash": "^4.17.10",
    "ncp": "^2.0.0",
    "rimraf": "^2.5.4",
    "puppeteer": "~1.11.0",
    "winston": "^0.9.0"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/node_eslintrc.js",
    "parser": "../chipper/node_modules/babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "globals": {
      "aqua": false,
      "XMLHttpRequest": false,
      "Hashes": false,
      "phetCore": false,
      "axon": false,
      "dot": false,
      "kite": false,
      "scenery": false,
      "__dirname": false
    }
  },
  "phet": {
    "buildStandalone": true,
    "standaloneTranspiles": [
      "../query-string-machine/js/QueryStringMachine.js"
    ]
  }
}